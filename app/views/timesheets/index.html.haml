.row
  %table.table.hoverable-row
    %thead
      %tr
        %th Period
        %th Vacation days
        %th Sickness days
        %th Army reserve days
        %th Leave of absence days
        %th 10Bis usage
        %th Status
        %th Comments
    %tbody
      - @timesheets.each do |timesheet|
        = render :partial => 'timesheets/timesheet', :locals => { :user => @user, :timesheet => timesheet }
